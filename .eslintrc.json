{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "react/jsx-filename-extension": "off",
    "react/destructuring-assignment": [
      "never"
    ],
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off",
    "no-plusplus": "off",
    "react/forbid-prop-types": "off"
  }
}